class Solution {
public:
string buildString(const string &s) {
    string result = "";
    for (char c : s) {
        if (c == '#') {
            if (!result.empty()) {
                result.pop_back(); // last character remove करो
            }
        } else {
            result.push_back(c); // letter add करो
        }
    }
    return result;
}
    bool backspaceCompare(string s, string t) {
          return buildString(s) == buildString(t);
  
    }
};
